    package="com.judopay">

    <uses-permission android:name="android.permission.INTERNET" />

    <application tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".PaymentActivity"
            android:theme="@style/Theme.Judo.Light.DarkActionBar" />
        <activity
            android:name=".PreAuthActivity"
            android:theme="@style/Theme.Judo.Light.DarkActionBar" />
        <activity
            android:name=".RegisterCardActivity"
            android:theme="@style/Theme.Judo.Light.DarkActionBar" />
        <activity
            android:name=".SaveCardActivity"
            android:theme="@style/Theme.Judo.Light.DarkActionBar" />
        <activity
            android:name=".CheckCardActivity"
